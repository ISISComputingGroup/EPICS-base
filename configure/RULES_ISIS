ifeq ($(STATIC_BUILD),YES)
ifneq ($(findstring linux,$(EPICS_HOST_ARCH)),)
MYSQLLIB = mysqlcppconn8-static mysqlcppconn-static
else
MYSQLLIB = mysqlcppconn8 mysqlcppconn
endif
USR_SYS_LIBS += mysqlclient
USR_LDFLAGS += -L/usr/lib64/mysql
else
ifneq ($(findstring linux,$(EPICS_HOST_ARCH)),)
MYSQLLIB = mysqlcppconn8 mysqlcppconn
else
MYSQLLIB = mysqlcppconn8 mysqlcppconn
endif
endif

